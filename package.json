{
  "name": "identitymatcher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 4000",
    "build": "next build",
    "start": "next start --port 4000",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "auth:migrate": "npx @better-auth/cli migrate",
    "db:seed": "npx tsx lib/db/scripts/seed.ts",
    "db:drop": "npx tsx lib/db/scripts/drop.ts",
    "db:init": "npx tsx lib/db/scripts/init.ts",
    "db:reset": "npm run db:drop && npm run db:init && npm run db:push && npm run db:seed"
  },
  "dependencies": {
    "@apollo/server": "^5.3.0",
    "@as-integrations/next": "^4.1.0",
    "@base-ui/react": "^1.2.0",
